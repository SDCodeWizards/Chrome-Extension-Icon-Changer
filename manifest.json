{
  "manifest_version": 3,
  "name": "Icon-changer",
  "description": "Changes other tabs on your chrome tabs. swap them to however you'd like",
  "permission": ["tabs", "storage", "activeTab"],
  "version": "1.0.0",
  "icons": {
    "128": "icons/icon.png"
  },
  "action": {
    "default_icon": {
      "32": "icons/icon.png"
    },
    "default_popup": "index.html",
    "default_title": "IDK..."
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+O",
        "mac": "MacCtrl+Shift+O"
      },
      "description": "Toggle Popup"
    }
  }

}
